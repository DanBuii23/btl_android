<resources>
    <!-- Base application theme. -->
    <style name="Theme.Baitaplon" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">@color/colorAccent</item>
    </style>

    <style name="Theme.Baitaplon.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Baitaplon.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.Baitaplon.PopuOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>